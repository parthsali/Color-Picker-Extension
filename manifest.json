{
    "name" : "Color Picker",
    "description" : "A Color Picker",
    "manifest_version": 3,
    "version": "1.0",
    "background" : {
        "service_worker": "background.js"
    },
    "permissions" : [
        "activeTab",
        "scripting"
    ],
    "action" :{
        "default_popup" : "popup.html"
    },
    "icons" :{
        "16": "images/icon16.png",
        "32" :"images/icon32.png",
        "48" : "images/icon48.png"
    }
}